# Set in stage2:
# ---------------------------------------------------------
# set -ef +am
# isFirstRun
# VAR_*
# All functions in /start/functions

if [ "$isFirstRun" == "true" ]
then
   if [ -n "$VAR_ROOT_APP" ] [ ! -e "$VAR_WEBAPPS_DIR/ROOT" ] && [ -d "$VAR_WEBAPPS_DIR/$VAR_ROOT_APP" ]
   then
      mv "$VAR_WEBAPPS_DIR/$VAR_ROOT_APP" "$VAR_WEBAPPS_DIR/ROOT"
   fi
   if [ "$VAR_WEBAPPS_DIR" != "/webapps-nobind" ]
   then
      rm -r /webapps-nobind
   fi
fi
